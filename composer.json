{
  "name": "akbv/php-skype",
  "description": "PHP library that wraps Skype Web API",
  "type": "library",
  "version": "1.2.4",
  "require": {
    "php": ">=7.3.21",
    "ext-json": "*",
    "ext-openssl": "*",
    "symfony/http-client": ">=4",
    "symfony/http-foundation": ">=4"
  },
  "require-dev": {
    "phpstan/phpstan": "~1.4",
    "friendsofphp/php-cs-fixer": "^3.3",
    "vlucas/phpdotenv": "^5.3",
    "monolog/monolog": "^2.5"
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix --dry-run --verbose",
    "php-cs-fixer-apply": "php-cs-fixer fix --verbose",
    "phpstan": [
      "phpstan analyse"
    ],
    "build": [
      "composer run-script php-cs-fixer",
      "composer run-script phpstan"
    ]
  },
  "keywords": [
    "skype",
    "client",
    "PHP",
    "openssl",
    "http"
  ],
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "Akbv\\PhpSkype\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Atanas Korabov"
    }
  ],
  "config": {
    "platform": {
      "php": "7.3.21"
    }
  },
  "minimum-stability": "dev"
}
